{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexitaylor/angular-graphql-nestjs-postgres-starter-kit"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 3000",
    "serve": "npx http-server ./dist -p 3000",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "keywords": [
    "angular-graphql-express-postgres-starter-kit",
    "angular",
    "Angular 8",
    "graphql",
    "server",
    "node",
    "express",
    "graphql",
    "apollo",
    "postgres",
    "boilerplate"
  ],
  "dependencies": {
    "@angular/animations": "11.1.1",
    "@angular/common": "11.1.1",
    "@angular/compiler": "11.1.1",
    "@angular/core": "11.1.1",
    "@angular/forms": "11.1.1",
    "@angular/http": "7.2.15",
    "@angular/localize": "^11.1.1",
    "@angular/platform-browser": "11.1.1",
    "@angular/platform-browser-dynamic": "11.1.1",
    "@angular/router": "11.1.1",
    "@angular/service-worker": "11.1.1",
    "@apollo/client": "^3.3.7",
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@ng-bootstrap/ng-bootstrap": "9.0.2",
    "@ngx-translate/core": "13.0.0",
    "@toverux/ngx-sweetalert2": "4.0.0",
    "@types/date-fns": "2.6.0",
    "@types/google-libphonenumber": "7.4.19",
    "@types/graphql": "14.2.3",
    "@types/ws": "7.4.0",
    "angulartics2": "10.0.0",
    "apollo-angular": "^2.2.0",
    "apollo-angular-link-http": "1.11.0",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-graphql": "^0.6.0",
    "apollo-link-context": "1.0.20",
    "apollo-link-error": "1.1.13",
    "apollo-link-rest": "^0.8.0-beta.0",
    "apollo-link-ws": "1.0.20",
    "bootstrap": "4.6.0",
    "core-js": "3.8.3",
    "date-fns": "2.16.1",
    "google-libphonenumber": "3.2.16",
    "graphql": "^15.5.0",
    "graphql-tag": "2.11.0",
    "lodash": "4.17.20",
    "npm-check-updates": "^11.1.1",
    "rxjs": "6.6.3",
    "subscriptions-transport-ws": "0.9.18",
    "sweetalert2": "10.14.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.2",
    "@angular/cli": "11.1.2",
    "@angular/compiler-cli": "11.1.1",
    "@angular/language-service": "11.1.1",
    "@biesbjerg/ngx-translate-extract": "7.0.3",
    "@ngx-rocket/scripts": "^5.0.0",
    "@types/jasmine": "3.6.3",
    "@types/jasminewd2": "2.0.8",
    "@types/lodash": "4.14.168",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.1",
    "hads": "3.0.0",
    "htmlhint": "0.14.2",
    "https-proxy-agent": "5.0.0",
    "husky": "4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.0.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-junit-reporter": "2.0.1",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "protractor": "~7.0.0",
    "puppeteer": "5.5.0",
    "stylelint": "13.9.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.18.0",
    "ts-node": "9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
